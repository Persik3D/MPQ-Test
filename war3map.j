function Trig_AnubMove_Actions takes nothing returns nothing
    call DisableTrigger( GetTriggeringTrigger() )
    call ShowUnitHide( udg_Boss1 )
    set udg_RandomNumber2 = GetRandomReal(0.00, 6.00)
    if ( Trig_AnubMove_Func004C() ) then
        set udg_AnubTempPoint = gg_rct_Anub1
    else
        if ( Trig_AnubMove_Func004Func001C() ) then
            set udg_AnubTempPoint = gg_rct_Anub2
        else
            if ( Trig_AnubMove_Func004Func001Func001C() ) then
                set udg_AnubTempPoint = gg_rct_Anub3
            else
                if ( Trig_AnubMove_Func004Func001Func001Func001C() ) then
                    set udg_AnubTempPoint = gg_rct_Anub4
                else
                    if ( Trig_AnubMove_Func004Func001Func001Func001Func001C() ) then
                        call MoveRectToLoc( udg_AnubTempPoint, GetUnitLoc(GroupPickRandomUnit(udg_HeroesGroup)) )
                    else
                    endif
                endif
            endif
        endif
    endif
    call ForGroupBJ( GetUnitsOfPlayerAndTypeId(Player(6), 'u005'), function Trig_AnubMove_Func005A )
    call ForGroupBJ( GetUnitsOfPlayerAndTypeId(Player(6), 'ucs3'), function Trig_AnubMove_Func006A )
    call ForGroupBJ( GetUnitsOfPlayerAndTypeId(Player(5), 'ucs3'), function Trig_AnubMove_Func007A )
    call TriggerSleepAction( 1.00 )
    call SetPlayerAbilityAvailableBJ( false, 'Abur', Player(6) )
    call TriggerSleepAction( 3.00 )
    call SetUnitPositionLoc( udg_Boss1, GetRectCenter(udg_AnubTempPoint) )
    call ForGroupBJ( udg_Spiders, function Trig_AnubMove_Func012002 )
    call SetUnitLifePercentBJ( udg_Boss1, ( GetUnitLifePercent(udg_Boss1) + 2.00 ) )
    call TriggerSleepAction( 4.00 )
    call SetPlayerAbilityAvailableBJ( true, 'Abur', Player(6) )
    call IssueImmediateOrderBJ( udg_Boss1, "unburrow" )
    call ForGroupBJ( udg_Spiders, function Trig_AnubMove_Func017002 )
    call GroupRemoveGroup( udg_Spiders, udg_Spiders )
    call IssueImmediateOrderBJ( udg_Boss1, "unburrow" )
    call ShowUnitShow( udg_Boss1 )
    call SetUnitAnimation( udg_Boss1, "morph defend" )
    call SetPlayerAbilityAvailableBJ( false, 'A01S', Player(5) )
    set udg_RandomNumber2 = GetRandomReal(0.00, 2.00)
    if ( Trig_AnubMove_Func024C() ) then
        call IssuePointOrderLocBJ( udg_Boss1, "attack", GetRectCenter(gg_rct________________002) )
    else
        call IssuePointOrderLocBJ( udg_Boss1, "attack", GetUnitLoc(GroupPickRandomUnit(udg_HeroesGroup)) )
    endif
    call TriggerSleepAction( 2 )
    call EnableTrigger( gg_trg_AnubDEBUG )
    call TriggerSleepAction( 15.00 )
    call EnableTrigger( gg_trg_AnubHide1 )
    call SetPlayerAbilityAvailableBJ( true, 'A01S', Player(5) )
    call EnableTrigger( GetTriggeringTrigger() )
endfunction

